  "description": "Study repo for LangChain, RAG, embeddings, and semantic vector search techniques",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
